{
  "name": "otd-ai-surfer-survivor-console",
  "version": "1.0.0",
  "description": "AI Surfer Survivor Console - A TypeScript-based web application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && npm start",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "surfer",
    "survivor",
    "console",
    "typescript",
    "express"
  ],
  "author": "Floridadoll1313",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Floridadoll1313/OTD-AI-SURFER-SURVIVOR-Console"
  }
}
